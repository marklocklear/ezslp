    <div id="container-fluid">
        <header>
            <div class="main-header navbar row-fluid">
                <div class="navbar-inner">
                    <a class="brand" href="/about"><%=image_tag("logo.png", alt: "ezSLP logo")%></a>
                    <ul class="nav nav-pills pull-right">
                        <li class="active">
                            <a id="blue-button" href="/register">REGISTER/LOGIN</a>
                        </li>
                        <li><form class="navbar-search pull-right">
                            <input type="text" class="search-query" placeholder="Search">
                        </form></li>
                        <li><a href=""><%=image_tag("twitter-square.png", alt:"Twitter logo")%></a></li>
                        <li><a href=""><%=image_tag("youtube-square.png", alt:"Youtube logo")%></a></li>
                    </ul>
                </div>
            </div>    
            <nav class="navbar row-fluid main-nav">
                <div class="navbar-inner">       
                    <ul class="nav nav-stacked pull-right main-nav">
                        <li class="<%= 'active' if current_page?(about_path) %>"><%= link_to "HOME", about_path%></li>
                        <li class="<%= 'active' if current_page?(contact_path) %>"><%= link_to "CONTACT", contact_path%></li>
                        <li class="<%= 'active' if current_page?(register_path) %>"><%= link_to "REGISTER", register_path%></li>
                        <li class="<%= 'active' if current_page?(screencasts_path) %>"><%= link_to "VIDEOS", screencasts_path%></li>
                    </ul>
                </div>   
            </nav>  
        </header>
        <div class="wrapper">
            <%= yield %>

	    </div><!-- end container-fluid div -->

    </div>           
    <footer>
        <div class="navbar navbar-default navbar-fixed-bottom">
            <div class="container">
                <p class="navbar-text pull-left">2018 - Site Built By Mark Locklear</p>
                <a href="https://www.youtube.com/user/ezslp" class="navbar-btn btn pull-right">
                    <%=image_tag("footeryoutube-square.png", alt:"Youtube logo", size: "45x45")%>
                </a>
                <a href="https://www.youtube.com/user/ezslp" class="navbar-btn btn pull-right">
                    <%=image_tag("footertwitter-square.png", alt:"Twitter logo", size: "45x45")%>
                </a>
                <a href="https://www.youtube.com/user/ezslp" class="navbar-btn btn pull-right">
                    <%=image_tag("footerenvelope.png", alt:"Email envelope logo", size: "60x45")%>
                </a>
            </div>
        </div>
    </footer>
